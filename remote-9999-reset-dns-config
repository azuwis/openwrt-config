echo

# chinadns
echo 'clean chinadns'
service stop chinadns || true
opkg_remove ChinaDNS
remove /tmp/dnsmasq.d/10-chinadns.conf
echo

# unbound
echo 'clean unbound'
opkg_remove unbound
rm -rf /etc/unbound/
echo

# chn-domains
echo 'clean chn-domains'
remove /etc/chn-domains /etc/hotplug.d/iface/99-chn-domains /tmp/dnsmasq.d/99-chn-domains.conf
echo

# pcap-dnsproxy
echo 'clean pcap-dnsproxy'
opkg_remove pcap-dnsproxy libsodium libpcap
remove /etc/pcap-dnsproxy
echo

service restart dnsmasq -
