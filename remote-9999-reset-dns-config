echo

# chinadns
echo 'clean chinadns'
service stop chinadns || true
opkg_remove ChinaDNS
rm -f /tmp/dnsmasq.d/10-chinadns.conf
echo

# unbound
echo 'clean unbound'
opkg_remove unbound
rm -rf /etc/unbound/
echo

# cn-domains
echo 'clean cn-domains'
remove /etc/chn-domains /etc/hotplug.d/iface/99-chn-domains /tmp/dnsmasq.d/99-chn-domains.conf
echo

service restart dnsmasq -
